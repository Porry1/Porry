  //  4. Вывести все числа от from до to. from, to - любые.
  //@author Porry
  package ua.Porry.HW4;

import java.util.Scanner;

public class HW4Two{
    public static void main(String[] args) {
    
        // // Input
        
        Scanner sca = new Scanner(System.in);

        System.out.println("do sume number; ");
        int from = sca.nextInt();
        System.out.println("do sume number2; ");
        int to = sca.nextInt();
        
        //Business Logic

        if (from <= to) {
            for (int i = from; i <= to; i++)
                System.out.println(i);
        } else {
            for (int i = from; i >= to; i--)
                System.out.println(i);
            
            }
        }
    }
}

