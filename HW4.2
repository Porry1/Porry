
//1. Вывести все числа от 10 до 30.
//@author Porry
package ua.Porry.HW4;

public class HW4Two{
    public static void main(String[] args) {
    
        // Input
        
        int from = 10;
        int to = 30;
        
        //Business Logic
        
        while  (from <= to)
        {
            System.out.println(from);
            from++;
        }

    }
}

//2. Вывести все числа от 20 до -40.
//@author Porry
package ua.Porry.HW4;

public class HW4Two{
    public static void main(String[] args) {
        // Input
        
        int from = 20;
        int to = -40;
        
        //Business Logic
        
        while  (from >= to)
        {
            System.out.println(from);
            from--;
        }

    }
}

